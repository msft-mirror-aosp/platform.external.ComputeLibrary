#endif  
#if M0 > 2
        ARM_DOT_K0XN0(K0, a2, b, c2);
#endif  
#if M0 > 3
        ARM_DOT_K0XN0(K0, a3, b, c3);
#endif  
#if M0 > 4
        ARM_DOT_K0XN0(K0, a4, b, c4);
#endif  
#if M0 > 5
        ARM_DOT_K0XN0(K0, a5, b, c5);
#endif  
#if M0 > 6
        ARM_DOT_K0XN0(K0, a6, b, c6);
#endif  
