#endif  
#if M0 > 6
        ARM_DOT_K0XN0(1, a6, b, c6);
#endif  
#if M0 > 7
        ARM_DOT_K0XN0(1, a7, b, c7);
#endif  

        lhs_offset += sizeof(DATA_TYPE);
        rhs_offset += sizeof(DATA_TYPE);
